.stickybox{
	padding: 10px 10px 0;
	font-size: .85em;
	width: 251px;
	height: 231px;
	position: relative;
	
}
	.stickybox::after {
	    width: 100%;
	    height: 30px;
	    content: '';
	    position: absolute;
	    bottom: -25px;
	    right: 0;
	}
	.stickybox.box-orange{
		background: #F36702;
		color: #fff;
	}
		.stickybox.box-orange::after {
			background: url(../images/sticky_corner_orange.png) no-repeat right;
		}
	.stickybox.box-purple{
		background: #8418A3;
		color: #fff;
	}
	.stickybox.box-green{
		background: #B5BD00;
		color: #fff;
	}

	.stickybox.box-green::after {
			background: url(../images/sticky_corner_green.png) no-repeat right;
	}
	
	.stickybox.box-red{
		background: #A50034;
		color: #fff;
	}

	.stickybox.box-red::after {
			background: url(../images/sticky_corner_red.png) no-repeat right;
		}
		
	.stickybox.box-purple{
		background: #8418A3;
		color: #fff;
	}
	.stickybox.box-purple::after {
			background: url(../images/sticky_corner_purple.png) no-repeat right;
		}
	.stickybox h3{
		font-size: 1.2em;
		font-weight: bold;
	}

