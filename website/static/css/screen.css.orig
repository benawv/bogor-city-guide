/* ------------------------------------------------------------------------------------------
	BASE
--------------------------------------------------------------------------------------------- */
body{
	text-align: center;
}
hr, .hide{
	display: none;
}
abbr, acronym{
	border-bottom: 1px dotted #999; cursor: help;
}
img{
	vertical-align: bottom;
	max-width: 100%;
}
* {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}

/* Clear Floated */
.clear {
	clear: both; font-size: 0; height: 0; line-height: 0; overflow: hidden;
}
.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}
.clearfix:after {
    clear: both;
}
/*** For IE 6/7 only Include this rule to trigger hasLayout and contain floats. */
.clearfix {
    *zoom: 1;
}


/* Link */
a{
	color: #17549c; text-decoration: none;
}
a:hover, a:active{
	color: #199dd7;
}

:-moz-placeholder{
	color: #999 !important;
	font-style: italic !important;
}
::-webkit-input-placeholder{
	color: #999 !important;
	font-style: italic !important;
}
::-moz-placeholder {  /* Firefox 19+ */
  color: #999 !important;
  font-style: italic !important;
}
:-ms-input-placeholder {
	color: #999 !important;
	font-style: italic !important;
}
::selection{
	background: rgba(95,135,173,0.9);
	color: #fff;
}
::-moz-selection {
    background: rgba(95,135,173,0.9);
	color: #fff;
}
::selection {
    background: rgba(95,135,173,0.9);
	color: #fff;
}



/* ------------------------------------------------------------------------------------------
	TYPOGRAPHY
--------------------------------------------------------------------------------------------- */
body{
	font-family: Arial, Helvetica, sans-serif;
	color: #111;
}
h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
	line-height: 1.5em;
	margin: 0;
	padding: 0;
}
strong{
	font-weight: normal;
}
em{
	font-style: italic;
}
input, textarea, select{
	font: 1.1em Arial, Helvetica, sans-serif;
}
.title {
	font-size: 24px;
	font-weight: normal;
	margin: 60px 0;
	padding: 0;
	display: block;
	-webkit-margin-before: 0.67em;
	-webkit-margin-after: 0.67em;
	-webkit-margin-start: 0px;
	-webkit-margin-end: 0px;
	font-weight: bold;
}



/* ------------------------------------------------------------------------------------------
	GENERAL
--------------------------------------------------------------------------------------------- */
img{
	max-width: 100%;
}
nav ul{
	margin: 0;
	padding: 0;
	list-style-type: none;
}



/* ------------------------------------------------------------------------------------------
